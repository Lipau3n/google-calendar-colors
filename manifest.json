{
  "name": "Google calendar colors",
  "description": "Changes google calendar colors",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://calendar.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}